<div class="admin-legodi">
    <table class="table bg-primary table-bordered  mt-4 pd-4">
        <thead class="table ">
            <tr class="text-center align-middle">
                <th>Id</th>
                <th>Name</th>
                <th>View</th>
             </tr>
        </thead>
        <tbody class="table bg-info text-white">
            {{#each data}}
            <tr>
                <th scope="row" class="text-center align-middle">{{this.article_id}}</th>
                <td class="text-center align-middle">{{this.title}}</td>
                <td class="text-center align-middle">
                    <a href='view/{{this.article_id}}'>
                       <button class="btn btn-sm eye"><i class="far fa-eye" ></i></button>                         
                    </a>
                </td>
            </tr>
            {{/each}}

        </tbody>
    </table>
</div>
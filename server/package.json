{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon --inspect ./bin/www",
    "lint": "standard",
    "test": "npm run migrate && jest .",
    "migrate": "knex migrate:latest && knex seed:run",
    "create-migration": "knex migrate:make $1",
    "create-seed": "knex seed:make $1",
    "cypress": "open cypress",
    "rollback": "knex migrate:rollback",
    "recreate-db": "npm run rollback && npm run migrate",
    "ci-recreate-db": "npm run rollback && npm run migrate"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "~1.18.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "^4.16.3",
    "express-fileupload": "^0.3.0",
    "express-messages": "^1.0.1",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "hbs": "~4.0.1",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.15.2",
    "mongodb": "^2.2.33",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "serve-favicon": "~2.4.5",
    "standard": "^11.0.1"
  },
  "devDependencies": {
    "jest": "^23.4.2",
    "nodemon": "^1.18.3",
    "supertest": "^3.1.0"
  },
  "standard": {
    "globals": [
      "describe",
      "expect",
      "it"
    ]
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  }
}
